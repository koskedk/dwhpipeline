                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select  distinct  Y.* 
from (
SELECT DISTINCT
      MFLCode, facname as FacilityName, [CT Partner],ageLVyrs as AgeAtLastVisit,
      Uniqueidno_orig as PatientID ,StartARTDate, firstARTreg as StartRegimen, firstARTline as StartRegimenLine,FirstDTGDispenseDate, LatestWeight,
      case when CurrentRegimen like '%DTG%' then CurrentRegimen else 'non DTG' end as LastRegimen,
       case when firstARTreg =CurrentRegimen then 'Initiation' else 'Transition' end as Category,
      dtLastVisit as DateLastVisit, dtlastapt as NextAppointmentDate, LastVL1 as LastVL, 
	  Case when ISNumeric(LastVL1)=1 and cast(Replace(Replace(LastVL1, ' ', ''), ',', '') as float) &gt;=1000.00 THEN 1 ELSE 0 END AS HighVL,dtlastVL1 as DateLastVL,EACVisitDate_1,EACVisitDate_2,EACVisitDate_3
 FROM (Select * from [All_Staging_2016_2].[dbo].[Cohort2015_2016](NoLock) where CurrentRegimen like '%DTG%' and ARTOutcome='V')c 

  LEFT JOIN (
    SELECT ROW_NUMBER () OVER (PARTITION BY stg_PatientPharmacy.PatientID, stg_PatientPharmacy.PatientPK, stg_PatientPharmacy.SiteCode, stg_PatientPharmacy.Drug ORDER BY DispenseDate ASC)  AS RowNumber, DispenseDate  FirstDTGDispenseDate, stg_PatientPharmacy.Drug, stg_PatientPharmacy.SiteCode, stg_PatientPharmacy.PatientID, stg_PatientPharmacy.PatientPk from All_Staging_2016_2.[dbo].stg_PatientPharmacy
    INNER JOIN (
        SELECT ROW_NUMBER () OVER (PARTITION BY PatientID, PatientPK, SiteCode ORDER BY DispenseDate DESC)  AS RowNumber, PatientID, PatientPK, SiteCode, Drug from All_Staging_2016_2.[dbo].stg_PatientPharmacy
    ) LatestDrug  on LatestDrug.PatientID = stg_PatientPharmacy.PatientID and LatestDrug.PatientPK = stg_PatientPharmacy.PatientPK and LatestDrug.SiteCode = stg_PatientPharmacy.SiteCode and LatestDrug.Drug = stg_PatientPharmacy.Drug and LatestDrug.RowNumber = 1
)As Pharm on c.MFLCode=Pharm.SiteCode and  c.uniqueidno_orig = Pharm.PatientID and c.PatientPk= Pharm.PatientPk and Pharm.RowNumber=1 

LEFT JOIN (SELECT * FROM (
SELECT SiteCode, PatientID, PatientPk, val,
Case  WHEN cols='EAC_VisitDate' THEN 'EACVisitDate_' WHEN cols='SessionNumber' THEN 'SessionNumber_' WHEN  cols='EACRecievedVL' THEN 'EACReceivedVL_'
    End + cast(DENSE_RANK() OVER( partition BY PatientID,SiteCode,PatientPK ORDER BY VisitDate ASC) as varchar)as NUM
FROM (
    SELECT SiteCode, PatientID, PatientPk ,VisitDate,
        CONVERT(VARCHAR(20),cast(VisitDate as date)) AS EAC_VisitDate,
       CONVERT(VARCHAR(20), [SessionNumber]) AS [SessionNumber],
       CONVERT(VARCHAR(20), [EACRecievedVL]) AS [EACRecievedVL]
      FROM All_Staging_2016_2.dbo.stg_EnhancedAdherenceCounselling WHERE Year(VisitDate)&gt;1900
    ) AS Source 
UNPIVOT
(
    val FOR cols IN (EAC_VisitDate,SessionNumber, EACRecievedVL)
) AS unpiv
)src
pivot(
MAX(val)
for NUM in (EACVisitDate_1,SessionNumber_1,EACReceivedVL_1,EACVisitDate_2,SessionNumber_2,EACReceivedVL_2,EACVisitDate_3,SessionNumber_3,EACReceivedVL_3)
) piv
) H on H.PatientPK=c.PatientPK and H.PatientID=c.uniqueidno_orig and H.SiteCode=c.MFLCode

LEFT  JOIN (SELECT * FROM (
              SELECT  TestName as TestNameHIVVL, 
         OrderedbyDate,ReportedByDate,TestResult,SiteCode, PatientID, PatientPk from All_Staging_2016_2.[dbo].[stg_PatientLabs]
             where    ([TestName] LIKE '%Viral%' OR TestName = 'VL' OR TestName like '%LDL%') AND TestResult IS NOT NULL 
             )A 
)AS AllLabs on c.MFLCode=AllLabs.SiteCode and  c.uniqueidno_orig = AllLabs.PatientID and c.PatientPk= AllLabs.PatientPk AND  AllLabs.OrderedbyDate&gt;= Pharm.FirstDTGDispenseDate 


   where MFLCode  in ('13468','13476','13488','11936','12881','11258','11259','18267','13507','11970','11976','12905','12904','15862','12929','12911','13220','12973',
   '18743',
'17719','13191','18219','16073','13604','13608','13939','12177','10485','13019','13667','13023','10520','10539','13028','10603','13703','13704','12366',
   '13050','16662','15965',
'13738','12438','12455','12457','13777','13781','12488','12492','13798','13805','16030','10777','10808','15282','10825','13841','13864',
   '10890','13921','10903','14012','13156',
'14020','13165','14058','10973','14080','14103','14106','14110','16141','14120','14121','14130','13074','11094','14156',
    '14175','12930')


)Y  Where  HighVL =1 and  FirstDTGDispenseDate &gt;= cast ('2017-07-01' as date)  and (StartRegimenLine not in ('Second Line','Third Line') or StartRegimenLine IS NULL) 


Order by PatientID, StartRegimenLine Desc</property>
